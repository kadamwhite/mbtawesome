{% if alerts.length %}
<button class="alert-list-toggle">
  {% for alert in alerts %}
  todo: {{ alert.severity | lower }} alert icon<br>
  {% endfor %}
</button>
<ul>
  {% for alert in alerts %}
  <li>
    {{ alert.alert_id }}
    ({{ alert.cause_name }}),
    {{ alert.severity }}:<br>
    {{ alert.header_text }}
  </li>
  {% endfor %}
</ul>
{% endif %}
