<h1>{{ name }}</h1>

<ul>
{% for station in stops %}
  {% if station.length %}
    {% for branch in station %}
    <li class="branch">
      <em>To {{ branch | last | get( 'name' ) }}</em>
      <ul>
        {% for station in branch %}
          <li class="station">{{ station.name }}</li>
        {% endfor %}
      </ul>
    </li>
    {% endfor %}
  {% else %}
    <li class="station">{{ station.name }}</li>
  {% endif %}
{% endfor %}
</ul>

<a href="/">Back to Home</a>
